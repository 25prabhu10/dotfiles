[user]
	name = Prabhu K H
	email = 25prabhu10@gmail.com
[color]
	ui = auto
[alias]
	a = add --all
	br = branch
	bs = branch -av
	ci = commit
	cm = commit --message
	ch = checkout
	dd = diff --check --dirstat --find-copies --find-renames --histogram --color
	l = log --pretty=oneline -n 20 --graph --abbrev-commit
	lg = log --pretty=format:\"%C(auto)%h %Cgreen%aN%Creset %s %Cgreen(%cr)%C(auto)%d\" --graph --all
	ls = ls-files
	last = log -1 HEAD
	rank = shortlog -sn --no-merges
	p = push
	remotes = remote -v
	s = status -sb
	st = status
	unstage = reset HEAD --
[core]
	editor = nvim
[github]
	user = 25prabhu10
